{
  "buildCommand": "prisma generate --schema prisma/schema.prisma && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30,
      "includeFiles": "node_modules/.prisma/client/**"
    },
    "app/api/**/*.js": {
      "maxDuration": 30,
      "includeFiles": "node_modules/.prisma/client/**"
    }
  }
}

